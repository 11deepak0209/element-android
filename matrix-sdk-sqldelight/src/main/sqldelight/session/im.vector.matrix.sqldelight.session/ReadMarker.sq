import java.lang.Boolean;

CREATE TABLE readMarkerEntity (
  room_id TEXT NOT NULL PRIMARY KEY,
  event_id TEXT NOT NULL
);

CREATE INDEX read_marker_room_id ON readMarkerEntity(room_id);

insertOrUpdate:
INSERT OR REPLACE INTO readMarkerEntity
VALUES ?;

get:
SELECT event_id FROM readMarkerEntity
WHERE room_id = :roomId;

deleteAll:
DELETE FROM readMarkerEntity;